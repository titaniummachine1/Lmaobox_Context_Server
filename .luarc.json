{
  "$schema": "https://raw.githubusercontent.com/LuaLS/vscode-lua/master/setting/schema.json",
  "runtime": {
    "version": "Lua 5.4",
    "path": ["?.lua", "?/init.lua"]
  },
  "diagnostics": {
    "enable": true,
    "globals": ["G"]
  },
  "workspace": {
    "library": [],
    "checkThirdParty": false
  },
  "completion": {
    "enable": true,
    "callSnippet": "Both"
  },
  "hint": {
    "enable": true
  }
}
